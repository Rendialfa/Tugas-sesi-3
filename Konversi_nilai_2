nilai = float(input("Masukkan nilai Anda: "))

if nilai < 50:
    print("Nilai Anda: E")
elif nilai >= 50 and nilai < 60:
    print("Nilai Anda: D")
elif nilai >= 60 and nilai < 70:
    print("Nilai Anda: C")
elif nilai >= 70 and nilai < 80:
    print("Nilai Anda: B")
else:
    print("Nilai Anda: A")
